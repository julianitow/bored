{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile:src": "babel src --out-dir ./dist",
    "compile": "npm run compile:src",
    "start": "npm run clean && npm run compile && electron app.js",
    "electron:version": "electron --version"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "electron": "^16.0.5",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.15",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "dotenv": "^10.0.0",
    "npx": "^10.2.2",
    "sass": "^1.45.0",
    "ssh2-sftp-client": "^7.2.1"
  }
}
